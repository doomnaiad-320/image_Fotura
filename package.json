{
  "name": "aigc-studio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "seed": "tsx prisma/seed.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:unit": "vitest run --config vitest.unit.config.ts",
    "test:e2e": "playwright test",
    "build:web": "cross-env BUILD_TARGET=web next build",
    "build:cap": "cross-env BUILD_TARGET=cap next build && next export --outdir dist-cap && npm run cap:copy",
    "cap:init": "npx cap init AIGCStudio com.example.aigcstudio --web-dir=dist-cap",
    "cap:copy": "npx cap copy",
    "cap:android": "npx cap open android",
    "cap:ios": "npx cap open ios"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@tanstack/react-query": "^5.28.2",
    "@prisma/client": "^5.15.0",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "jose": "^5.3.0",
    "lru-cache": "^10.2.0",
    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "openai": "^4.52.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-hook-form": "^7.51.5",
    "tailwind-merge": "^2.2.2",
    "swr": "^2.2.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.0",
    "@tailwindcss/forms": "^0.5.7",
    "@capacitor/cli": "^5.7.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/lru-cache": "^5.1.1",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prisma": "^5.15.0",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
